# Database (PostgreSQL)
DATABASE_URL=postgres://user:password@localhost:5432/sneakdrop

# Server
PORT=3001
NODE_ENV=development

# Optional: comma-separated origins for Socket.io CORS (e.g. https://your-app.vercel.app). If unset, all origins are allowed.
# CORS_ORIGIN=https://your-app.vercel.app

# Reservation expiry window (seconds)
RESERVATION_TTL_SECONDS=60

# Job interval for releasing expired reservations (ms)
RESERVATION_EXPIRY_JOB_INTERVAL_MS=5000
